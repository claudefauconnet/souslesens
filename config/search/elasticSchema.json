            },
            "url": {
              "type": "text",
              "inCSV": true
            },
            "content": {
              "type": "text",
              "index_options": "offsets",
              "analyzer": "ATD",
              "fielddata": true,
              "fields": {
                "contentKeyWords": {
                  "type": "keyword",
                  "ignore_above": 256
                },
                "sensitive": {
                  "fielddata": true,
                  "type": "text",
                  "analyzer": "whitespace"
