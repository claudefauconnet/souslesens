  "_settings": {
    "ATD": {
      "analysis": {
        "filter": {
          "stemmer_filter": {
            "type": "stemmer",
            "language": "french"
          }
        },
        "analyzer": {
          "ATD": {
            "tokenizer": "standard",
            "filter": [
              "lowercase",
              "asciifolding",
              "stemmer_filter"
            ]
          }
        }
      }
    },
    "BASIC": {
      "settings": {
        "analysis": {
          "analyzer": {
            "BASIC": {
              "tokenizer": "standard",
              "filter": [
                "lowercase",
                "asciifolding"
              ]
            }
          }
        }
      },
      "mappings": {
        "doc": {
          "dynamic_templates": [
            {
              "objectField": {
                "path_match": "*",
                "mapping": {
                  "analyzer": "BASIC"
                }
              }
            }
          ]
        }
      }
    },
    "SYNONYMS_FR": {
      "analysis": {
        "filter": {
          "synonyms_fr_filter": {
            "type": "synonym",
            "synonyms_path": "synonyms/wordNetSyns.txt"
          },
          "stemmer_filter": {
            "type": "stemmer",
            "language": "french"
          }
        },
        "analyzer": {
          "SYNONYMS": {
            "tokenizer": "standard",
            "filter": [
              "lowercase",
              "asciifolding",
              "stemmer_filter"
            ]
          }
        }
      }
    }
  },
  "_indexes": {
    "web_page": {
      "mappings": {
        "web_page": {
